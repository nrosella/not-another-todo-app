<% if @list.is_completed == false %>
	<li class="item" data-id-second="<%= @list.id %>">
		<div class="panel panel-info box-shadow--8dp">
		  <div class="panel-heading">
		    <h5 class="panel-header-title">Mission Critical Task Two</h5>
		  </div>
		  <div class="panel-body">
				<h4 class="panel-p-tag" id="item-two-complete"><%= @list.item_two %></h4>
		  </div>
		</div>
	</li>
<% else %>
	<li class="item" data-id-second="<%= @list.id %>">
		<div class="panel panel-info box-shadow--8dp">
		  <div class="panel-heading">
		    <h5 class="panel-header-title">Mission Critical Task Two</h5>
		  </div>
		  <div class="panel-body">
				<h4 class="panel-p-tag strikethrough" id="item-two-complete"><%= @list.item_two %></h4>
		  </div>
		</div>
	</li>
<% end %>

